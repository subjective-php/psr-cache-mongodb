{
    "name": "subjective-php/psr-cache-mongodb",
    "description": "PSR-16 SimpleCache Implementation using MongoDB",
    "keywords": ["psr", "psr16", "mongo", "cache"],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "mongodb/mongodb": "^1.1",
        "psr/simple-cache": "^1.0",
        "subjective-php/psr-cache-helper": "^1.1.1"
    },
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": { "SubjectivePHP\\Psr\\SimpleCache\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SubjectivePHPTest\\Psr\\SimpleCache\\": "tests" }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "test": "vendor/bin/phpunit"
    }
}
